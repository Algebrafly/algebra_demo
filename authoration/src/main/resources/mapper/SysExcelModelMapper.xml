<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.algebra.authentication.mapper.excel.SysExcelModelMapper">
  <resultMap id="BaseResultMap" type="com.algebra.authentication.domain.SysExcelModel">
    <!--@mbg.generated-->
    <!--@Table sys_excel_model-->
    <id column="id" jdbcType="CHAR" property="id" />
    <result column="name" jdbcType="VARCHAR" property="name" />
    <result column="code" jdbcType="CHAR" property="code" />
    <result column="business_key" jdbcType="VARCHAR" property="businessKey" />
    <result column="app_key" jdbcType="VARCHAR" property="appKey" />
    <result column="store_type" jdbcType="CHAR" property="storeType" />
    <result column="oss_url" jdbcType="VARCHAR" property="ossUrl" />
    <result column="deleted" jdbcType="BOOLEAN" property="deleted" />
  </resultMap>
  <sql id="Base_Column_List">
    <!--@mbg.generated-->
    id, `name`, code, business_key, app_key, store_type, oss_url, deleted
  </sql>
  <select id="getModelsPage" resultType="com.algebra.authentication.domain.SysExcelModel">
    SELECT
    <include refid="Base_Column_List" />
    FROM
      `sys_excel_model`
    WHERE
      deleted = 0
    <if test="name != null">
      AND name like CONCAT('%',#{name,jdbcType=VARCHAR},'%')
    </if>
    <if test="appKey != null">
      AND app_key like CONCAT('%',#{appKey,jdbcType=VARCHAR},'%')
    </if>
    <if test="businessKey != null">
      AND business_key like CONCAT('%',#{businessKey,jdbcType=VARCHAR},'%')
    </if>
    <if test="code != null">
      AND code like CONCAT('%',#{code,jdbcType=VARCHAR},'%')
    </if>
  </select>
</mapper>
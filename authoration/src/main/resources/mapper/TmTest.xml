<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.algebra.authentication.mapper.config.TmTestMapper">

    <select id="getListForPage" resultType="com.algebra.authentication.domain.TmTest">
        SELECT * FROM tm_aa where 1=1 order by inputtime LIMIT 10 OFFSET 1
    </select>

    <select id="getInParam" parameterType="int" resultType="String">
        SELECT id FROM tm_aa LIMIT ${limit}
    </select>

</mapper>